<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.flashlearn.HomePageController">
   <BorderPane prefHeight="400.0" prefWidth="600.0">
      <center>
         <ScrollPane fx:id="contentHolder" prefHeight="350.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <FlowPane fx:id="buttonHolder" prefWidth="600.0" >
                  <Button fx:id="addSetButton" onAction="#addSet" prefHeight="100.0" prefWidth="100.0">+</Button>
               </FlowPane>
            </ScrollPane>
      </center>
      <top>
         <HBox prefHeight="100.0" prefWidth="200.0" spacing="100.0" BorderPane.alignment="CENTER">
            <Button fx:id="logOutButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="68.0" text="Log Out" onAction="#logOut"/>
            <Text text="Home Page" textAlignment="CENTER">
               <font>
                  <Font size="50.0" />
               </font>
            </Text>
         </HBox>
      </top>
   </BorderPane>
</AnchorPane>
