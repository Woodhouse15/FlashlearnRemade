<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.flashlearn.SignUpController"
            prefHeight="400.0" prefWidth="600.0">
    <Text layoutX="184.0" layoutY="133.0" text="Flashlearn" wrappingWidth="222.00390625">
        <font>
            <Font size="50.0" />
        </font></Text>
    <Button fx:id="signUpButton" layoutX="266.0" layoutY="316.0" mnemonicParsing="false" text="Sign Up" onAction="#signUp"/>
    <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Back" onAction="#back"/>
    <PasswordField fx:id="passwordField" layoutX="220.0" layoutY="245.0" promptText="Enter New Password" />
    <TextField fx:id="usernameField" layoutX="220.0" layoutY="200.0" promptText="Enter New Username" />
    <Text fx:id="signUpGuide" layoutX="220.0" layoutY="290.0"/>
</AnchorPane>
